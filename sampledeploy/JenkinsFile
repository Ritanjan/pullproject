pipeline {
   agent any
   enviorment {
       ANYPOINT = credentials('ANYPOINT')
   }

   stages {
      stage('Build') {
         steps {
            withMaven(maven:'maven'){
                sh 'mvn -f sampledeploy/pom.xml clean install'
            }
         }
     stage('Deploy') {
         steps {
            withMaven(maven:'maven'){
                sh 'mvn -f sampledeploy/pom.xml package deploy -Dusername=Arpita1992 -Dpassword=Arpita1992 -Denviorment=Development -DmuleDeploy'
            }
         }
      }
   }
}
