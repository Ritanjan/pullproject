pipeline {
   agent any
   environment {
       ANYPOINT = credentials('ANYPOINT')
   }
   stages {
      stage('Build') {
         steps {
            withMaven(maven:'maven'){
                sh 'mvn -f sampledeploy/pom.xml clean'
            }
         }
		 }
     stage('Deploy') {
         steps {
            withMaven(maven:'maven'){
                 sh 'mvn -f sampledeploy/pom.xml clean'
            }
         }
      }
   }
}