pipeline {
   agent any
   environment {
       ANYPOINT = credentials('ANYPOINT')
   }
   stages {
      stage('Build') {
         steps {
             withMaven(maven:'maven'){
                sh 'mvn clean'
            }
         }
		 }
     stage('Deploy') {
         steps {
             withMaven(maven:'maven'){
                sh 'mvn clean'
            }
            }
         }
      }
   }
}